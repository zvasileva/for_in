{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
	 "indent": [2, "tab"],
     "no-tabs": 0,
     "no-plusplus": 0,
	 "array-callback-return": 0,
	 "consistent-return": 0
  }
}